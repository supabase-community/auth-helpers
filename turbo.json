{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", ".svelte-kit/**", ".remix/**"]
    },
    "build:example": {
      "dependsOn": ["^build", "^build:example"],
      "outputs": ["dist/**", ".next/**", ".svelte-kit/**", ".remix/**"]
    },
    "build:watch": {
      "dependsOn": ["^build:watch"],
      "outputs": ["dist/**", ".next/**", ".svelte-kit/**", ".remix/**"]
    },
    "clean:all": {
      "dependsOn": ["^clean:all"],
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig.json", ".env.*"]
}
