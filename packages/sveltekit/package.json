{
  "name": "@supabase/auth-helpers-sveltekit",
  "version": "0.0.0",
  "description": "A collection of framework specific Auth utilities for working with Supabase.",
  "files": [
    "package"
  ],
  "types": "./package/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./package/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm package",
    "prepare": "svelte-kit sync",
    "package": "svelte-kit package && rm package/package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supabase-community/auth-helpers.git"
  },
  "keywords": [
    "Supabase",
    "Auth",
    "Svelte Kit",
    "Svelte"
  ],
  "author": "Supabase Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/supabase-community/auth-helpers/issues"
  },
  "homepage": "https://github.com/supabase-community/auth-helpers#readme",
  "devDependencies": {
    "@supabase/supabase-js": "^1.35.3",
    "@sveltejs/kit": "next",
    "config": "workspace:*",
    "shared": "workspace:*",
    "svelte": "^3.48.0",
    "svelte2tsx": "^0.5.10",
    "tslib": "^2.3.1",
    "tsup": "^5.12.5",
    "typescript": "^4.6.3",
    "jose": "^4.8.1"
  }
}