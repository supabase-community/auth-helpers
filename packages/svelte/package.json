{
    "name": "@supabase/auth-helpers-svelte",
    "version": "0.1.0",
    "types": "./dist/index.d.ts",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        "./package.json": "./package.json",
        ".": "./dist/index.js"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "pnpm copy:shared && pnpm package && pnpm rm:shared",
        "prepare": "svelte-kit sync",
        "package": "svelte-kit package && rimraf dist/package.json",
        "copy:shared": "copyfiles -s ../shared/** src/shared -e ../shared/*.md -e ../shared/*.json",
        "rm:shared": "rimraf src/shared"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/supabase-community/supabase-auth-helpers.git"
    },
    "keywords": [
        "Supabase",
        "Auth",
        "Svelte Kit",
        "Svelte"
    ],
    "author": "Supabase Community",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/supabase-community/supabase-auth-helpers/issues"
    },
    "homepage": "https://github.com/supabase-community/supabase-auth-helpers#readme",
    "devDependencies": {
        "@sveltejs/kit": "next",
        "copyfiles": "^2.4.1",
        "rimraf": "^3.0.2",
        "svelte": "^3.48.0",
        "svelte-preprocess": "^4.10.1",
        "svelte2tsx": "^0.5.10",
        "tslib": "^2.3.1",
        "typescript": "^4.7.2"
    },
    "dependencies": {
        "@supabase/supabase-js": "^1.35.3"
    }
}