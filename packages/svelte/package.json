    "devDependencies": {
        "@sveltejs/kit": "next",
        "del-cli": "^4.0.1",
        "svelte": "^3.48.0",
        "svelte-preprocess": "^4.10.1",
        "svelte2tsx": "^0.5.10",
        "tslib": "^2.3.1",
        "typescript": "^4.7.2"
